<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

    <title>{% block title %}Memonote{% endblock %}</title>
        
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('css/jquery-ui.min.css') }}" type="text/css" />
        <link rel="stylesheet" href="{{ asset('css/shCore.css') }}" type="text/css" />
        <link rel="stylesheet" href="{{ asset('css/shThemeDefault.css') }}" type="text/css" />
        <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}" type="text/css" />
        <link rel="stylesheet" href="{{ asset('bundles/jclememo/css/style.css') }}" type="text/css" />
    {% endblock %}
      
  </head>

<body>
    
    <div class="container">
        <div class="row">
            <h1 class="text-center"><a href="{{ path("jclememo_accueil") }}" tabindex="-1">Memonote</a></h1> 
        </div>
    
        {% block body %}
            {% block jclememo_navbar %}{% endblock %}
            {% block jclememo_body %}{% endblock %}
        {% endblock %}

        {% block javascript %}
            <script src="{{ asset('js/jquery.js') }}" type="text/javascript" ></script>
            <script src="{{ asset ('js/jquery-ui.min.js') }}" type="text/javascript"></script>
{#            <script src="{{ asset('js/syntaxHighlighter/shAutoloader.js') }}" type="text/javascript" ></script>#}
            <script src="{{ asset('bundles/jclememo/js/memo.js') }}" type="text/javascript" ></script>
            <script src="{{ asset('js/shCore.js') }}" type="text/javascript" ></script>
{#            <script src="{{ asset('js/brushesSH/shBrushPhp.js') }}" type="text/javascript" ></script>
            <script src="{{ asset('js/brushesSH/shBrushCSharp.js') }}" type="text/javascript" ></script>#}
            <script src="{{ asset('js/bootstrap.min.js') }}" type="text/javascript" ></script>
            <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
            <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
            <script type="text/javascript">
                var pathImgDirectory = "{{ asset('uploads/icon/') }}";
{#                SyntaxHighlighter.all();#}
                SyntaxHighlighter.config.stripBrs = true;
           </script>
           <script type="text/javascript" src="{{ asset('js/shCore.js') }}"></script>
            <script type="text/javascript" src="{{ asset('js/brushesSH/shAutoloader.js') }}" ></script>
            <script type="text/javascript">
                SyntaxHighlighter.autoloader(
                ['js','jscript','javascript','{{ asset('js/brushesSH/shBrushJScript.js') }}'],
                ['c#','csharp','{{ asset('js/brushesSH/shBrushCSharp.js') }}'],
                ['css','{{ asset('js/brushesSH/shBrushCss.js') }}'],
                ['xml','{{ asset('js/brushesSH/shBrushXml.js') }}'],
                ['sql','{{ asset('js/brushesSH/shBrushSql.js') }}'],
                ['php','{{ asset('js/brushesSH/shBrushPhp.js') }}']
                );
                SyntaxHighlighter.all();
            </script>
        {% endblock %}
    
{#    <footer><p>Nom récupéré dans le parameters et config = {{ webmaster }} @ 2014</p></footer>#}
    </div>
    
  </body>
</html>